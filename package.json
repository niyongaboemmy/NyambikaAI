{
  "name": "nyambika-ai",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run dev:python\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "dev:python": "cd python/clothflow_service && python -m uvicorn main:app --reload --port 8000",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
    "start:frontend": "cd frontend && npm run preview",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "check": "cd frontend && npm run check && cd ../backend && npm run check",
    "db:push": "cd backend && npm run db:push",
    "seed": "cd backend && npm run seed"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.85.5",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12"
  },
  "devDependencies": {
    "@types/node": "20.16.11"
  }
}
